\begin_layout Subsubsection
H8 
\begin_inset Formula $\to$
\end_inset

 P5
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
N_{a}^{H}=\frac{1}{8}(1+\xi_{a}\xi)(1+\eta_{a}\eta)(1+\zeta_{a}\zeta),\quad a=1,2,3,4,5,6,7,8,
\]

\end_inset

前 4 点不动：
\begin_inset Formula 
\[
N_{a}^{H\to P}=N_{a}^{H}=\frac{1}{8}(1+\xi_{a}\xi)(1+\eta_{a}\eta)(1+\zeta_{a}\zeta),\quad a=1,2,3,4,
\]

\end_inset

后 4 点合并：
\begin_inset Formula 
\[
N_{a}^{H\to P}=\sum_{a=5}^{8}N_{a}^{H}=\frac{1+\zeta}{2}\sum_{a=5}^{8}\frac{(1+\xi_{a}\xi)(1+\eta_{a}\eta)}{4}=\frac{1+\zeta}{2},\quad a=5,
\]

\end_inset


\end_layout

\begin_layout Subsubsection
H20/H27 
\begin_inset Formula $\to$
\end_inset

 P13/P14
\end_layout

\begin_layout Standard
顶点的 hierarchical 形函数为
\begin_inset Formula 
\[
N_{a}^{H}=\frac{1}{8}(1+\xi_{a}\xi)(1+\eta_{a}\eta)(1+\zeta_{a}\zeta),\quad a=1,2,3,4,5,6,7,8,
\]

\end_inset

相应的 hierarchical 插值为
\begin_inset Formula 
\[
u^{h,8}(\vec{r})=\sum_{a=1}^{8}u(\vec{r}_{a})\cdot N_{a}^{H}(\vec{r}),
\]

\end_inset

此时 hierarchical 形函数亦即 Lagrange 形函数，故上述插值可记为
\begin_inset Formula 
\[
u^{h,8}(\vec{r})=\sum_{a=1}^{8}\hat{u}_{a}\cdot N_{a}^{H8}(\vec{r}),\quad\begin{cases}
\hat{u}_{a}=u(\vec{r}_{a}), & a=1,2,3,4,5,6,7,8,\\
N_{a}^{H8}(\vec{r})=N_{a}^{H}(\vec{r}), & a=1,2,3,4,5,6,7,8,
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
引入边点，其 hierarchical 形函数为
\begin_inset Formula 
\[
N_{a}^{H}=\frac{1}{4}\begin{cases}
(1-\xi^{2})(1+\eta_{a}\eta)(1+\zeta_{a}\zeta), & a=9,11,13,15,\quad\xi_{a}=0,\\
(1+\xi_{a}\xi)(1-\eta^{2})(1+\zeta_{a}\zeta), & a=10,12,14,16,\quad\eta_{a}=0,\\
(1+\xi_{a}\xi)(1+\eta_{a}\eta)(1-\zeta^{2}), & a=17,18,19,20,\quad\zeta_{a}=0,
\end{cases}
\]

\end_inset

相应的 hierarchical 插值为
\begin_inset Formula 
\[
\begin{aligned}u^{h,20}(\vec{r}) & =u^{h,8}(\vec{r})+\sum_{b=9}^{20}\left(u(\vec{r}_{b})-u^{h,8}(\vec{r}_{b})\right)\cdot N_{b}^{H}(\vec{r})\\
 & =\sum_{a=1}^{8}\hat{u}_{a}\cdot N_{a}^{H8}(\vec{r})+\sum_{b=9}^{20}\left(u(\vec{r}_{b})-\sum_{a=1}^{8}\hat{u}_{a}\cdot N_{a}^{H8}(\vec{r}_{b})\right)\cdot N_{b}^{H}(\vec{r})\\
 & =\sum_{a=1}^{8}\hat{u}_{a}\cdot\left(N_{a}^{H8}(\vec{r})-\sum_{b=9}^{20}N_{a}^{H8}(\vec{r}_{b})\cdot N_{b}^{H}(\vec{r})\right)+\sum_{b=9}^{20}\hat{u}_{b}\cdot N_{b}^{H}(\vec{r}),
\end{aligned}
\]

\end_inset

最后一行即 Lagrange 插值，可记为
\begin_inset Formula 
\[
u^{h,20}(\vec{r})=\sum_{a=1}^{20}\hat{u}_{a}\cdot N_{a}^{H20}(\vec{r}),\quad\begin{cases}
\hat{u}_{a}=u(\vec{r}_{a}), & a=1,2,\dots,20,\\
N_{a}^{H20}(\vec{r})=N_{a}^{H8}(\vec{r})-\sum_{b=9}^{20}N_{a}^{H8}(\vec{r}_{b})\cdot N_{b}^{H}(\vec{r}), & a=1,2,\dots,8,\\
N_{a}^{H20}(\vec{r})=N_{a}^{H}(\vec{r}), & a=9,10,\dots,20,
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
进一步引入面点
\begin_inset Formula 
\[
N_{a}^{H}=\frac{1}{2}\begin{cases}
(1+\xi_{a}\xi)(1-\eta^{2})(1-\zeta^{2}), & a=21,22,\quad\eta_{a}=\zeta_{a}=0,\\
(1-\xi^{2})(1+\eta_{a}\eta)(1-\zeta^{2}), & a=23,24,\quad\xi_{a}=\zeta_{a}=0,\\
(1-\xi^{2})(1-\eta^{2})(1+\zeta_{a}\zeta), & a=25,26,\quad\xi_{a}=\eta_{a}=0,
\end{cases}
\]

\end_inset

相应的 hierarchical 插值为
\begin_inset Formula 
\[
\begin{aligned}u^{h,26}(\vec{r}) & =u^{h,20}(\vec{r})+\sum_{b=21}^{26}\left(u(\vec{r}_{b})-u^{h,20}(\vec{r}_{b})\right)\cdot N_{b}^{H}(\vec{r})\\
 & =\sum_{a=1}^{20}\hat{u}_{a}\cdot\left(N_{a}^{H20}(\vec{r})-\sum_{b=21}^{26}N_{a}^{H20}(\vec{r}_{b})\cdot N_{b}^{H}(\vec{r})\right)+\sum_{b=21}^{26}\hat{u}_{b}\cdot N_{b}^{H}(\vec{r}),
\end{aligned}
\]

\end_inset

最后一行即 Lagrange 插值
\begin_inset Formula 
\[
u^{h,26}(\vec{r})=\sum_{a=1}^{26}\hat{u}_{a}\cdot N_{a}^{H26}(\vec{r}),\quad\begin{cases}
\hat{u}_{a}=u(\vec{r}_{a}), & a=1,2,\dots,26,\\
N_{a}^{H26}(\vec{r})=N_{a}^{H20}(\vec{r})-\sum_{b=21}^{26}N_{a}^{H20}(\vec{r}_{b})\cdot N_{b}^{H}(\vec{r}), & a=1,2,\dots,20,\\
N_{a}^{H26}(\vec{r})=N_{a}^{H}(\vec{r}), & a=21,22,\dots,26,
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
最后引入内点
\begin_inset Formula 
\[
N_{a}^{H}=(1-\xi^{2})(1-\eta^{2})(1-\zeta^{2}),\quad a=27,\quad\xi_{a}=\eta_{a}=\zeta_{a}=0,
\]

\end_inset

相应的 hierarchical 插值为
\begin_inset Formula 
\[
\begin{aligned}u^{h,27}(\vec{r}) & =u^{h,26}(\vec{r})+\left(u(\vec{r}_{27})-u^{h,26}(\vec{r}_{27})\right)\cdot N_{27}^{H}(\vec{r})\\
 & =\sum_{a=1}^{26}\hat{u}_{a}\cdot\left(N_{a}^{H26}(\vec{r})-N_{a}^{H26}(\vec{r}_{27})\cdot N_{27}^{H}(\vec{r})\right)+\hat{u}_{27}\cdot N_{27}^{H}(\vec{r}),
\end{aligned}
\]

\end_inset

最后一行即 Lagrange 插值
\begin_inset Formula 
\[
u^{h,27}(\vec{r})=\sum_{a=1}^{27}\hat{u}_{a}\cdot N_{a}^{H27}(\vec{r}),\quad\begin{cases}
\hat{u}_{a}=u(\vec{r}_{a}), & a=1,2,\dots,27,\\
N_{a}^{H27}(\vec{r})=N_{a}^{H26}(\vec{r})-N_{a}^{H26}(\vec{r}_{27})\cdot N_{27}^{H}(\vec{r}), & a=1,2,\dots,26,\\
N_{a}^{H27}(\vec{r})=N_{a}^{H}(\vec{r}), & a=27,
\end{cases}
\]

\end_inset


\end_layout

\begin_layout Standard
合并一条边，需在其两侧分别做 Q8/Q9 
\begin_inset Formula $\to$
\end_inset

 T3 或 T6 
\begin_inset Formula $\to$
\end_inset

 L3 退化。例如：合并 5-6，需忽略 5-6 边上的 13、用 26 修正对侧的 15，用 23 修正对侧的 9。故合并顶部 4 点、忽略面点，即
\begin_inset Formula 
\[
a\to\begin{cases}
a', & a=1,2,3,4,\\
5', & a=5,6,7,8,\\
(a-3)', & a=9,10,11,12,\\
(a-7)', & a=17,18,19,20,\\
14', & a=25,
\end{cases}
\]

\end_inset

所得 Pyramid 单元的形函数为
\end_layout

\begin_layout Itemize
未合并的顶点
\begin_inset Formula 
\[
N_{1'}^{P}=N_{1}^{H},\quad N_{2'}^{P}=N_{2}^{H},\quad N_{3'}^{P}=N_{3}^{H},\quad N_{4'}^{P}=N_{4}^{H},
\]

\end_inset


\end_layout

\begin_layout Itemize
被合并的顶点
\begin_inset Formula 
\[
N_{5'}^{P}=N_{5}^{H}+N_{6}^{H}+N_{7}^{H}+N_{8}^{H},
\]

\end_inset


\end_layout

\begin_layout Itemize
需修正的边点
\begin_inset Formula 
\[
N_{6'}^{P}=N_{9}^{H}-\frac{1}{4}N_{23}^{H},\quad N_{7'}^{P}=N_{10}^{H}-\frac{1}{4}N_{22}^{H},\quad N_{8'}^{P}=N_{11}^{H}-\frac{1}{4}N_{24}^{H},\quad N_{9'}^{P}=N_{12}^{H}-\frac{1}{4}N_{21}^{H},
\]

\end_inset


\end_layout

\begin_layout Itemize
未受影响的顶点
\begin_inset Formula 
\[
N_{10'}^{P}=N_{17}^{H},\quad N_{11'}^{P}=N_{18}^{H},\quad N_{12'}^{P}=N_{19}^{H},\quad N_{13'}^{P}=N_{20}^{H},
\]

\end_inset


\end_layout

\begin_layout Itemize
未受影响的面点（仅 H20、P14 有）
\begin_inset Formula 
\[
N_{14'}^{P}=N_{25}^{H},
\]

\end_inset


\end_layout

